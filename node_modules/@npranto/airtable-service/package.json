{
  "name": "@npranto/airtable-service",
  "version": "1.0.17",
  "description": "A Node.js package that makes it easy to use Airtable REST API for CRUD operations",
  "type": "module",
  "source": "index.js",
  "exports": {
    "require": "./dist/air-service.cjs",
    "default": "./dist/air-service.modern.js"
  },
  "main": "dist/air-service.cjs",
  "module": "dist/air-service.esm.js",
  "unpkg": "dist/air-service.umd.js",
  "scripts": {
    "build": "microbundle",
    "dev": "microbundle watch",
    "publish:patch": "npm version patch && npm publish --access public",
    "publish:minor": "npm version minor && npm publish --access public",
    "publish:major": "npm version major && npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/npranto/airtable-service.git"
  },
  "keywords": [
    "airtable",
    "service",
    "nsp"
  ],
  "author": "Nazmuz Pranto <npranto@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/npranto/airtable-service/issues"
  },
  "homepage": "https://github.com/npranto/airtable-service#readme",
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "axios": "^1.3.5",
    "dotenv": "^16.0.3",
    "ms": "^2.0.0"
  },
  "devDependencies": {
    "microbundle": "^0.15.1"
  },
  "files": [
    "dist"
  ]
}
